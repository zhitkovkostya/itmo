<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>UnicodeSymbols</servlet-name>
		<servlet-class>com.UnicodeSymbols</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ClosestNumber</servlet-name>
		<servlet-class>com.ClosestNumber</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>QuadraticEquation</servlet-name>
		<servlet-class>com.QuadraticEquation</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>RandomArray</servlet-name>
		<servlet-class>com.RandomArray</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>UnicodeSymbols</servlet-name>
		<url-pattern>/UnicodeSymbols</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ClosestNumber</servlet-name>
		<url-pattern>/ClosestNumber</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>QuadraticEquation</servlet-name>
		<url-pattern>/QuadraticEquation</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>RandomArray</servlet-name>
		<url-pattern>/RandomArray</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
